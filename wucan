sjzdzb = false

Tab:AddToggle({
	Name = "自动哑铃锻炼",
	Default = false,
	Callback = function(Value)
		sjzdzb = Value
		game:GetService("RunService").Stepped:connect(
		function()
			pcall(
				function()
					if sjzdzb then
					game:GetService("ReplicatedStorage").Events.Weight:FireServer()
						if sjzdzb then
							game.Players.LocalPlayer.Character.Humanoid:EquipTool(
								game:GetService("Players").LocalPlayer.Backpack.Weight
							)
						end
					end
				end
			)
		end
		)
	end    
})

